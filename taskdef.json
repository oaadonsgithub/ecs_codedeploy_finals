{
    "family": "karrio-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::537124950459:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "karrio",
        "image": "<IMAGE_URI>",
        "portMappings": [
          {
            "containerPort": 3000,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ]
  }
  